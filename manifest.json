{
  "manifest_version": 3,
  "name": "Sticky Noter",
  "version": "1.0",
  "description": "Add sticky notes to any webpage",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
